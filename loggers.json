{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "default": {
            "format": "[%(levelname)-10s] [модуль:%(name)22s] %(asctime)-25s - %(message)s"
        }
    },
    "handlers": {
        "file": {
            "class": "logging.FileHandler",
            "level": "INFO",
            "formatter": "default",
